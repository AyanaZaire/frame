<h1>Create User</h1>

  <%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :bio %>
    <%= f.text_area :bio %>

    <%= f.label :user_img_url %>
    <%= f.text_field :user_img_url %>

    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "Create New User" %>
  <% end %>
